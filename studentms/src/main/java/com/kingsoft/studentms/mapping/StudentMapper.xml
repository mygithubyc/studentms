<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.kingsoft.studentms.dao.IStudentDao">
	<resultMap type="com.kingsoft.studentms.model.Student" id="Student">
		<result column="STUDENT_ID" property="studentId" jdbcType="INTEGER" />
		<result column="USER_ID" property="userId" jdbcType="INTEGER" />
		<result column="DEPART_ID" property="departId" jdbcType="INTEGER" />
		<result column="CLASS_ID" property="classId" jdbcType="INTEGER" />
		<result column="ENTRANCE_TIME" property="entranceTime" jdbcType="DATE" />
		<result column="SCHOOL_YEAR" property="schoolYear" jdbcType="INTEGER" />
	</resultMap>
	
	<insert id="dAddStudent">
		INSERT INTO STUDENT (
			USER_ID,
			DEPART_ID,
			CLASS_ID,
			ENTRANCE_TIME,
			SCHOOL_YEAR
		)
		VALUES
			(#{userId},#{departId},#{classId},#{entranceTime},#{schoolYear})
	</insert>
</mapper>